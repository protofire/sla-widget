{
  "plugins": ["prettier-plugin-embed"],
  "htmlWhitespaceSensitivity": "ignore",
  "semi": true,
  "singleQuote": true,
  "printWidth": 80,
  "tabWidth": 2,
  "trailingComma": "all",
  "arrowParens": "always",
  "bracketSpacing": true,
  "proseWrap": "always",
  "embeddedLanguageFormatting": "auto"
}
